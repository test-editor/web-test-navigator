{
  "name": "@testeditor/test-navigator",
  "version": "0.2.9",
  "description": "Angular component providing a top level navigation view for test editor project",
  "license": "EPL",
  "keywords": [
    "test-editor",
    "angular",
    "test-navigator"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/test-editor/web-test-navigator"
  },
  "contributors": [
    "Jan Jelschen",
    "Gunther Bachmann"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "clean": "rimraf out-tsc dist/*",
    "upgradeTesteditorOrgDependencies": "yarn upgrade -S @testeditor",
    "ng": "ng",
    "prestart": "yarn run upgradeTesteditorOrgDependencies",
    "start": "ng serve --host 0.0.0.0",
    "prebuild": "yarn run upgradeTesteditorOrgDependencies && yarn run clean && yarn run lint",
    "build": "ng build",
    "pretest": "yarn run upgradeTesteditorOrgDependencies",
    "test": "ng test",
    "pretest:once": "yarn run upgradeTesteditorOrgDependencies",
    "test:once": "ng test --watch=false",
    "lint": "ng lint",
    "pree2e": "webdriver-manager update --ignore_ssl --gecko=false",
    "e2e": "ng e2e --webdriver-update=false",
    "packagr": "ng-packagr -p ng-package.json",
    "postversion": "git push && git push --tags"
  },
  "COMMENT_scripts_test": [
    "as described in issue https://github.com/angular/angular-cli/issues/7296",
    "source maps have to be turned off for ng cli (--source-map=false)",
    "for legible error messages in some cases (e.g. html template access missing function)"
  ],
  "peerDependencies": {
    "@testeditor/messaging-service": "^1.5.7",
    "@testeditor/testeditor-commons": "~0.6.5",
    "bootstrap": ">=4.4.1",
    "font-awesome": ">=4.7.0",
    "ngx-bootstrap": "^5.5.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.5",
    "@angular-devkit/build-ng-packagr": "~0.900.5",
    "@angular/animations": "^9.0.5",
    "@angular/cli": "^9.0.5",
    "@angular/common": "^9.0.5",
    "@angular/compiler": "^9.0.5",
    "@angular/compiler-cli": "^9.0.5",
    "@angular/core": "^9.0.5",
    "@angular/forms": "^9.0.5",
    "@angular/http": "^7.2.16",
    "@angular/language-service": "^9.0.5",
    "@angular/platform-browser": "^9.0.5",
    "@angular/platform-browser-dynamic": "^9.0.5",
    "@angular/router": "^9.0.5",
    "@testeditor/messaging-service": "~1.5.0",
    "@testeditor/testeditor-commons": "~0.6.5",
    "@types/jasmine": "~3.5.7",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~13.7.7",
    "bootstrap": "^4.3.1",
    "caniuse-lite": "1.0.30000997",
    "codelyzer": "^5.2.1",
    "core-js": "^3.6.4",
    "font-awesome": "4.7.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-firefox-launcher": "1.3.0",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.2",
    "ng-packagr": "^9.0.2",
    "ngx-bootstrap": "^3.2.0",
    "protractor": "~5.4.3",
    "rimraf": "3.0.2",
    "rxjs": "^6.5.4",
    "ts-mockito": "^2.5.0",
    "ts-node": "~8.6.2",
    "tsickle": "^0.38.1",
    "tslib": "^1.11.1",
    "tslint": "6.0.0",
    "typescript": "~3.7.4",
    "zone.js": "^0.10.2"
  },
  "dependencies": {
    "@angular/localize": "^9.0.5"
  }
}
